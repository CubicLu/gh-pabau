{
  "extends": ["plugin:@nrwl/nx/react", "../../.eslintrc.js"],
  "ignorePatterns": ["!**/*", "**/node_modules"],
  "overrides": [
    {
      "files": ["*.tsx", "*.ts"],
      "rules": { "@typescript-eslint/ban-ts-comment": "off" }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": { "@typescript-eslint/no-var-requires": "off" }
    }
  ]
}
