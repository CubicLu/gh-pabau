  "workspaces": {
    "packages": [
      "apps/bridge-proxy"
    ],
    "nohoist": [
      "bridge-proxy/**"
    ]
  },
  "scripts": {
    "nx": "cross-env NODE_OPTIONS='--max-old-space-size=4096' nx",
    "vercel": "vercel",
    "apollo": "npx apollo",
    "nest": "cd apps/backend && nest",
    "clean": "rimraf **/node_modules",
    "web:start": "nx serve web",
    "start:linux": "yarn dev:linux",
    "start:windows": "yarn dev:windows",
    "dev:linux": "concurrently --kill-others-on-fail -n HASURA,PRISMA,WEB,WEB-PRELOADER,LAUNCH-BROWSER -c red,green,blue,grey,grey \"delay 5 && yarn hasura:stop && docker-compose -f hasura/docker-compose.linux.yml up -d postgres && wait-on tcp:4000 && yarn hasura:start:linux\" \"yarn bridge:start\" \"yarn web:start\" \"delay 15 && wait-on tcp:4000 && wait-on http://localhost:4200\" \"delay 20 && wait-on http://localhost:4200 && wait-on http-get://localhost:8080/console && open-cli 'http://localhost:4200'\"",
    "dev:windows": "concurrently --kill-others-on-fail -n HASURA,PRISMA,WEB,WEB-PRELOADER,LAUNCH-BROWSER -c red,green,blue,grey,grey \"delay 5 && docker-compose -f hasura/docker-compose.windows.yml up -d postgres && wait-on tcp:4000 && yarn hasura:start:windows\" \"yarn bridge:start\" \"yarn web:start\" \"delay 15 && wait-on tcp:4000 && wait-on http://localhost:4200\" \"delay 20 && wait-on http://localhost:4200 && wait-on http-get://localhost:8080/console && open-cli 'http://localhost:4200'\"",
    "bridge:start": "nx serve bridge-api --prod",
    "hasura:start:linux": "docker-compose -f hasura/docker-compose.linux.yml up -d postgres && delay 10 && docker-compose -f hasura/docker-compose.linux.yml up --force-recreate graphql-engine",
    "hasura:start:windows": "docker-compose -f hasura/docker-compose.windows.yml up -d postgres && delay 10 && docker-compose -f hasura/docker-compose.windows.yml up --force-recreate graphql-engine",
    "hasura:stop": "docker-compose -f hasura/docker-compose.linux.yml down -t 5",
    "hasura:clean": "docker-compose -f hasura/docker-compose.linux.yml down -v",
    "hasura:export": "env-cmd -f ./hasura/.env npx -p hasura-cli@2.0.0-alpha.7 hasura --project hasura metadata export && apollo client:download-schema remote-schema.graphql && apollo client:download-schema graphql.schema.json",
    "production:start": "nx affected --uncommitted=true --target=export && npx serve dist/apps/web/exported",
    "build": "pwd && ls -al && rm -rf public/ && nx build backend --no-cache && cp -rf dist/apps/backend/ public/ && cp apps/backend/vercel.json public/ && cp package.json public/",
    "pipeline-checks": "yarn --pure-lockfile --prefer-offline && yarn lint:tsc && yarn nx affected --all --target=lint && yarn nx affected --all --target=test && yarn nx affected --all --target=build",
    "pipeline-checks:parallel": "yarn --pure-lockfile --prefer-offline && concurrently --max-processes=5 --kill-others-on-fail -n TSC,LS-LINT,LINT,TEST,BUILD,E2E \"yarn lint:tsc\" \"yarn ls-lint || true\" \"yarn nx affected --all --target=lint\" \"yarn nx affected --all --target=test\" \"yarn nx affected --all --target=build\"",
    "pipeline-checks:insane": "yarn --pure-lockfile --prefer-offline && concurrently --max-processes=6 -n TSC,LS-LINT,LINT,TEST,BUILD,E2E \"yarn lint:tsc\" \"yarn ls-lint || true\" \"yarn nx affected --all --target=lint\" \"yarn nx affected --all --target=test\" \"yarn nx affected --all --target=build\"",
    "lint:tsc": "concurrently --max-processes=5 \"tsc --noEmit -p apps/backend/\" \"tsc --noEmit -p apps/connect/\" \"tsc --noEmit -p apps/connect-e2e/\" \"tsc --noEmit -p apps/mgmt/\" \"tsc --noEmit -p apps/mgmt-e2e/\" \"tsc --noEmit -p apps/ui-e2e/\" \"tsc --noEmit -p apps/web/\" \"tsc --noEmit -p apps/web-e2e/\" \"tsc --noEmit -p libs/ui/tsconfig.lib.json\" \"tsc --noEmit -p libs/yup/tsconfig.lib.json\"",
    "lint:ls": "ls-lint",
    "lint:workspace": "nx workspace-lint",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:export": "nx affected --target=export",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "yarn upgrade-interactive && nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "nexus:generate": "ts-node-dev --transpile-only apps/bridge-api/src/schema",
    "bridge:generate": "prisma format && prisma generate && yarn pal g && yarn pal schema typescript -o apps/bridge-api/src/generated && yarn nexus:generate",
    "bridge:prune": "rm -rf apps/bridge-api/src/generated",
    "postinstall": "concurrently -n HUSKY,PRISMA -c yellow,green \"husky install\" \"prisma generate\""
  },
  "resolutions": {
    "**/immer": "8.0.1",
    "**/prismjs": "1.23.0",
