  "scripts": {
    "nx": "cross-env NODE_OPTIONS=\"--max-old-space-size=6543\" nx",
    "serve": "yarn nx serve",
    "build:bridge-api": "yarn nx export bridge-api",
    "hasura:cli": "ts-node tools/cli/hasura.ts",
    "hasura:refresh-metadata": "yarn hasura:cli metadata inconsistency status || ts-node tools/cli/hasura-refresh-metadata.ts",
    "hasura:wait": "ts-node tools/cli/hasura-wait.ts",
    "nest": "cd apps/backend && nest",
    "clean": "rimraf **/node_modules yarn.lock package-lock.json dist coverage",
    "web:start": "yarn nodemon -x \"yarn nx serve || shx touch apps/web/next.config.js\" -e '' -w 'apps/web/next.config.js'",
    "connect:start": "nx serve connect --port=4400",
    "start": "concurrently -m 16 --kill-others-on-fail -n WEB,LAUNCH-BROWSER \"yarn web:start\" \"wait-on tcp:localhost:4200 && echo App web is ready but now warming up the root page... && echo Will automatically launch your browser when done && wait-on http://localhost:4200 && open-cli \\\"http://web.localhost:4200\\\"\"",
    "dev": "concurrently -m 16 --kill-others-on-fail -n DOCKER,PRISMA,HASURA-METADATA -c yellow,red,green,grey,blue,grey,grey,magenta \"yarn mysql:start && yarn hasura:start\" \"yarn bridge:start2\" \"wait-on -w 1200 \\\"http-get://localhost:4000\\\" && echo BRIDGE NOW ALIVE && wait-on -w 1200 \\\"http-get://localhost:8080/console\\\" && echo HASURA AND BRIDGE ALIVE && yarn hasura:refresh-metadata && yarn hasura:cli console\"",
    "dev:connect": "concurrently -m 16 --kill-others-on-fail -n HASURA,HASURA-SEED,PRISMA,WEB,WEB-PRELOADER,LAUNCH-BROWSER,BACKEND -c red,red,green,blue,grey,grey,magenta \"delay 5 && yarn hasura:stop && docker-compose -f hasura/docker-compose.yml up -d postgres && wait-on -w 1200 http-get://localhost:4000 && yarn hasura:start\" \"delay 22 && wait-on -w 1200 http-get://localhost:8080/console && (yarn hasura:cli seeds apply --database-name default || echo 'Seed apply failed') && open-cli 'http://localhost:8080' && wait-on http://localhost:4200 && open-cli 'http://localhost:4200'\" \"yarn bridge:start\" \"delay 25 && yarn connect:start\" \"delay 15 && wait-on tcp:4000 && wait-on http://localhost:4200\" \"delay 20 && wait-on http://localhost:4200\" \"yarn nx serve backend\"",
    "dev:light": "concurrently \"yarn nx serve web\" \"yarn nx serve bridge-api\"",
    "bridge:start": "yarn nx serve bridge-api",
    "bridge:start2": "node tools/cli/esbuild.cjs && yarn env-cmd -f apps/bridge-api/.env node dist/asdf/main.cjs",
    "mysql:start": "docker-compose -f tools/pabau1/docker-compose.yml up -d || (docker-compose -f tools/pabau1/docker-compose.yml down && docker-compose -f tools/pabau1/docker-compose.yml up -d)",
    "postgres:start": "docker-compose -f hasura/docker-compose.yml up -d postgres",
    "hasura:start": "run-script-os",
    "hasura:start:default": "docker-compose -f hasura/docker-compose.yml up -d postgres && docker-compose -f hasura/docker-compose.yml up -d graphql-engine",
    "hasura:start:linux": "docker-compose -f hasura/docker-compose.yml up -d postgres && docker-compose -f hasura/docker-compose.yml -f hasura/docker-compose.linux.yml up graphql-engine",
    "hasura:stop": "docker-compose -f hasura/docker-compose.yml down -t 15",
    "hasura:clean": "docker-compose -f hasura/docker-compose.yml down -v",
    "mysql:clean": "docker-compose -f tools/pabau1/docker-compose.yml down -v",
    "hasura:export": "yarn hasura:cli metadata export && yarn graphql-codegen --config codegen.introspect-localhost.yml && yarn graphql-codegen:cached",
    "graphql-codegen:cached": "postmon --include hasura/schema.graphql apps/bridge-api/src/generated/nexus.d.ts \"libs/graphql/**/*.graphql\" -- yarn graphql-codegen",
    "graphql-codegen:watch": "graphql-codegen --watch",
    "graphql-codegen:clean": "rimraf libs/graphql/generated",
    "graphql-codegen": "yarn graphql-codegen:clean && graphql-codegen",
    "production:web": "env-cmd -f apps/web/.env.production yarn nx serve",
    "production:start": "yarn nx affected --uncommitted=true --target=export && npx serve dist/apps/web/exported",
    "pipeline-checks": "yarn --pure-lockfile --prefer-offline && yarn lint:workspace && yarn nx affected --all --target=tsc --parallel && yarn nx affected --all --target=lint --parallel",
    "pipeline-checks:extended": "yarn --pure-lockfile --prefer-offline && yarn lint:workspace && yarn nx affected --all --target=tsc --parallel && yarn nx affected --all --target=lint --parallel && yarn nx affected --all --target=test --parallel && yarn nx affected --all --target=e2e",
    "lint:tsc": "yarn nx affected --target=tsc",
    "lint:ls": "ls-lint",
    "lint:workspace": "yarn nx workspace-lint",
    "test": "yarn nx test",
    "lint": "yarn lint:workspace && yarn nx affected:lint --parallel",
    "e2e": "yarn nx e2e",
    "affected:apps": "yarn nx affected:apps",
    "affected:libs": "yarn nx affected:libs",
    "affected:build": "yarn nx affected:build",
    "affected:export": "yarn nx affected --target=export",
    "affected:e2e": "yarn nx affected:e2e",
    "affected:test": "yarn nx affected:test",
    "affected:lint": "yarn nx affected:lint",
    "affected:dep-graph": "yarn nx affected:dep-graph",
    "affected": "yarn nx affected",
    "format": "yarn nx format:write",
    "format:write": "yarn nx format:write",
    "format:check": "yarn nx format:check",
    "update": "yarn upgrade-interactive && yarn nx migrate latest",
    "workspace-schematic": "yarn nx workspace-schematic",
    "dep-graph": "yarn nx dep-graph",
    "nexus:generate": "node tools/cli/esbuild.cjs && yarn env-cmd -f apps/bridge-api/.env node dist/asdf/bridge-api-nexus.cjs --nexus-typegen && ls -al dist/asdf/ && cp -f dist/asdf/generated/nexus.d.ts apps/bridge-api/src/generated/",
    "bridge:generate": "prisma generate && prisma format && node tools/cli/convert-file-to-lf.js apps/bridge-api/src/prisma/schema.prisma && rimraf libs/graphql/crm-mutation && yarn dotenv-flow -p apps/bridge-api pal g && concurrently -n NEXUS,GRAPHQL-CODEGEN -c blue,yellow,green \"yarn nexus:generate\" \"yarn graphql-codegen:cached\"",
    "bridge:clean": "rimraf apps/bridge-api/src/generated libs/graphql/crm-mutation",
    "bridge:prune": "yarn bridge:clean",
    "lint:graphql-inspector": "yarn graphql-inspector validate --noStrictFragments --deprecated \"{libs,apps}/**/*.graphql\" hasura/schema.graphql",
    "load-testing": "ts-node tools/cicd/load-testing",
    "postinstall": "patch-package --patch-dir tools/patches && husky install && yarn graphql-codegen:cached || (yarn lint:graphql-inspector && exit 1)"
  },
