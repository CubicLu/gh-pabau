  "scripts": {
    "nx": "cross-env NODE_OPTIONS=\"--max-old-space-size=6543\" nx",
    "serve": "yarn nx serve",
    "vercel": "vercel",
    "apollo": "ts-node tools/cli/apollo.ts",
    "build:bridge-api": "yarn nx export bridge-api",
    "hasura:cli": "ts-node tools/cli/hasura.ts",
    "hasura:refresh-metadata": "ts-node tools/cli/hasura-refresh-metadata.ts",
    "nest": "cd apps/backend && nest",
    "clean": "rimraf **/node_modules yarn.lock package-lock.json",
    "web:start": "yarn nodemon -x \"yarn nx serve || shx touch apps/web/next.config.js\" -e '' -w 'apps/web/next.config.js'",
    "start:linux": "yarn dev:linux",
    "start:windows": "yarn dev:windows",
    "dev": "concurrently -m 16 --kill-others-on-fail -n GRAPHQL-CODEGEN,MYSQL,HASURA,PRISMA,HASURA-METADATA,WEB,WEB-PRELOADER,LAUNCH-BROWSER,BACKEND -c yellow,red,green,grey,blue,grey,grey,magenta \"yarn graphql-codegen:watch\" \"yarn mysql:start\" \"yarn hasura:start\" \"yarn bridge:start\" \"wait-on -w 1200 http-get://localhost:4000 && echo BRIDGE NOW ALIVE && wait-on -w 1200 http-get://localhost:8080/console && echo HASURA AND BRIDGE ALIVE && yarn hasura:cli metadata reload && yarn hasura:refresh-metadata && yarn hasura:cli console\" \"yarn web:start\" \"wait-on tcp:4000 && wait-on -w 1200 http-get://localhost:8080/console && wait-on http://localhost:4200 && open-cli \\\"http://web.localhost:4200\\\"\" \"wait-on http://localhost:4200\" \"yarn nx serve backend\"",
    "dev:light": "concurrently \"yarn nx serve web\" \"yarn nx serve bridge-api\"",
    "bridge:start": "yarn prisma generate && yarn nx serve bridge-api",
    "mysql:start": "docker-compose -f tools/pabau1/docker-compose.yml up -d || (docker-compose -f tools/pabau1/docker-compose.yml down && docker-compose -f tools/pabau1/docker-compose.yml up -d)",
    "hasura:start": "run-script-os",
    "hasura:start:default": "docker-compose -f hasura/docker-compose.yml up -d postgres && docker-compose -f hasura/docker-compose.yml up -d graphql-engine",
    "hasura:start:linux": "docker-compose -f hasura/docker-compose.yml up -d postgres && docker-compose -f hasura/docker-compose.yml -f hasura/docker-compose.linux.yml up graphql-engine",
    "hasura:stop": "docker-compose -f hasura/docker-compose.yml down -t 5",
    "hasura:clean": "docker-compose -f hasura/docker-compose.yml down -v",
    "mysql:clean": "docker-compose -f tools/pabau1/docker-compose.yml down -v",
    "hasura:export": "yarn hasura:cli metadata export && yarn apollo client:download-schema hasura/schema.graphql && yarn graphql-codegen",
    "graphql-codegen": "graphql-codegen",
    "graphql-codegen:watch": "nodemon -w libs/graphql -x graphql-codegen -e graphql",
    "production:web": "env-cmd -f apps/web/.env.production yarn nx serve",
    "production:start": "yarn nx affected --uncommitted=true --target=export && npx serve dist/apps/web/exported",
    "build": "pwd && ls -al && rm -rf public/ && nx build backend --no-cache && cp -rf dist/apps/backend/ public/ && cp apps/backend/vercel.json public/ && cp package.json public/",
    "pipeline-checks": "yarn --pure-lockfile --prefer-offline && yarn lint:workspace && yarn nx affected --all --target=tsc --parallel --skip-nx-cache && yarn nx affected --all --target=lint --parallel --skip-nx-cache && yarn nx affected --all --target=test --parallel --skip-nx-cache",
    "pipeline-checks:slow": "yarn --pure-lockfile --prefer-offline && yarn lint:tsc && yarn lint:workspace && yarn nx affected --all --target=lint && yarn nx affected --all --target=test && yarn nx affected --all --target=build",
    "pipeline-checks:parallel": "yarn --pure-lockfile --prefer-offline && concurrently --max-processes=3 --kill-others-on-fail -n TSC,NX,LS-LINT,LINT,TEST,BUILD,E2E \"yarn lint:tsc\" \"yarn lint:workspace\" \"yarn ls-lint || echo TODO\" \"yarn nx affected --all --target=lint --parallel\" \"yarn nx affected --all --target=test --parallel\"",
    "pipeline-checks:insane": "yarn --pure-lockfile --prefer-offline && concurrently --max-processes=4 -n TSC,NX,LS-LINT,LINT,TEST,BUILD,E2E \"yarn lint:tsc\" \"yarn lint:workspace\" \"yarn ls-lint || echo TODO\" \"yarn nx affected --all --target=lint --parallel\" \"yarn nx affected --all --target=test --parallel\" \"yarn nx affected --all --target=build --parallel\"",
    "lint:tsc": "yarn nx affected --target=tsc",
    "lint:ls": "ls-lint",
    "lint:workspace": "yarn nx workspace-lint",
    "test": "yarn nx test",
    "lint": "yarn lint:workspace && yarn nx affected:lint --parallel",
    "e2e": "yarn nx e2e",
    "affected:apps": "yarn nx affected:apps",
    "affected:libs": "yarn nx affected:libs",
    "affected:build": "yarn nx affected:build",
    "affected:export": "yarn nx affected --target=export",
    "affected:e2e": "yarn nx affected:e2e",
    "affected:test": "yarn nx affected:test",
    "affected:lint": "yarn nx affected:lint",
    "affected:dep-graph": "yarn nx affected:dep-graph",
    "affected": "yarn nx affected",
    "format": "yarn nx format:write",
    "format:write": "yarn nx format:write",
    "format:check": "yarn nx format:check",
    "update": "yarn upgrade-interactive && yarn nx migrate latest",
    "workspace-schematic": "yarn nx workspace-schematic",
    "dep-graph": "yarn nx dep-graph",
    "nexus:generate": "ts-node-dev --transpile-only apps/bridge-api/src/schema --nexus-typegen",
    "bridge:generate": "rm -rf libs/graphql/crm-mutation && prisma format && prisma generate && yarn pal g && yarn nexus:generate",
    "bridge:prune": "rm -rf apps/bridge-api/src/generated libs/graphql/crm-mutation",
    "prepare": "husky install"
  },
