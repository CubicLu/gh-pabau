{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve web",
      "command": "yarn serve web",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve bridge-api",
      "command": "yarn serve bridge-api",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve bridge-proxy",
      "command": "yarn serve bridge-proxy",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve backend",
      "command": "yarn serve backend",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve mgmt",
      "command": "yarn serve mgmt",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve connect",
      "command": "yarn serve connect",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve ui",
      "command": "yarn serve ui:storybook",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "serve web",
      "command": "yarn serve web",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "command": "yarn pipeline-checks:parallel",
      "name": "pipeline-checks:parallel",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "yarn pipeline-checks:insane",
      "name": "pipeline-checks:insane",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "pipeline-checks",
      "request": "launch",
      "runtimeArgs": ["run-script", "pipeline-checks"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "dev",
      "request": "launch",
      "runtimeArgs": ["serve", "web"],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nx",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true,
      "type": "pwa-node",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug bridge-api",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/apps/bridge-api/main.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "preLaunchTask": "npm: build:bridge-api",
      "outFiles": ["dist/"]
    }
  ]
}
