{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "yarn pipeline-checks:parallel",
      "name": "pipeline-checks:parallel",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "yarn pipeline-checks:insane",
      "name": "pipeline-checks:insane",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "pipeline-checks",
      "request": "launch",
      "runtimeArgs": ["run-script", "pipeline-checks"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "bridge-api",
      "request": "launch",
      "runtimeArgs": ["serve", "bridge-api"],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nx",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true,
      "type": "pwa-node",
      "console": "integratedTerminal",
      "outputCapture": "std",
      "preLaunchTask": "npm: build:bridge-api"
    },
    {
      "name": "bridge-proxy",
      "request": "launch",
      "runtimeArgs": ["serve", "bridge-proxy"],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nx",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true,
      "type": "pwa-node",
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "name": "dev",
      "request": "launch",
      "runtimeArgs": ["serve", "web"],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nx",
      "skipFiles": ["<node_internals>/**"],
      "autoAttachChildProcesses": true,
      "type": "pwa-node",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug bridge-api (build first)",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/apps/bridge-api/main.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "preLaunchTask": "npm: build:bridge-api",
      "outFiles": ["dist/"]
    }
  ]
}
